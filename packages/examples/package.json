{
    "name": "@systembug/wsx-examples",
    "version": "0.0.4",
    "description": "Example applications using WSX Framework",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:pages": "cross-env NODE_ENV=production GITHUB_PAGES=true vite build",
        "preview": "vite preview",
        "deploy:pages": "pnpm build:pages && gh-pages -d dist",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest watch",
        "test:ui": "vitest --ui",
        "typecheck": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "@systembug/wsx-core": "workspace:*",
        "@systembug/wsx-base-components": "workspace:*",
        "@systembug/wsx-router": "workspace:*",
        "@unocss/preset-attributify": "^66.3.3",
        "@unocss/preset-uno": "^66.3.3"
    },
    "devDependencies": {
        "@systembug/eslint-plugin-wsx": "workspace:*",
        "@systembug/wsx-vite-plugin": "workspace:*",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/user-event": "^14.6.1",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitest/ui": "^3.2.4",
        "cross-env": "^7.0.3",
        "eslint": "^8.0.0",
        "gh-pages": "^6.1.1",
        "happy-dom": "^18.0.1",
        "typescript": "^5.0.0",
        "unocss": "^66.3.3",
        "vite": "^5.0.0",
        "vitest": "^3.2.4"
    }
}
