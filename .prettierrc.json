{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "proseWrap": "preserve",
    "overrides": [
        {
            "files": "*.wsx",
            "options": {
                "parser": "typescript"
            }
        }
    ]
}
